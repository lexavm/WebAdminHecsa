@model IEnumerable<WebAdminHecsa.Models.TblClienteContacto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
             <td style="text-align:center">
                @Html.DisplayNameFor(model => model.NombreClienteContacto)
            </th>
             <td style="text-align:center">
                @Html.DisplayNameFor(model => model.CorreoElectronico)
            </th>
             <td style="text-align:center">
                @Html.DisplayNameFor(model => model.Telefono)
            </th>
             <td style="text-align:center">
                @Html.DisplayNameFor(model => model.IdCliente)
            </th>
             <td style="text-align:center">
                @Html.DisplayNameFor(model => model.NombreCliente)
            </th>
             <td style="text-align:center">
                @Html.DisplayNameFor(model => model.FechaRegistro)
            </th>
             <td style="text-align:center">
                @Html.DisplayNameFor(model => model.IdEstatusRegistro)
            </th>
             <td style="text-align:center"></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td style="text-align:center">
                @Html.DisplayFor(modelItem => item.NombreClienteContacto)
            </td>
            <td style="text-align:center">
                @Html.DisplayFor(modelItem => item.CorreoElectronico)
            </td>
            <td style="text-align:center">
                @Html.DisplayFor(modelItem => item.Telefono)
            </td>
            <td style="text-align:center">
                @Html.DisplayFor(modelItem => item.IdCliente)
            </td>
            <td style="text-align:center">
                @Html.DisplayFor(modelItem => item.NombreCliente)
            </td>
            <td style="text-align:center">
                @Html.DisplayFor(modelItem => item.FechaRegistro)
            </td>
            <td style="text-align:center">
                @Html.DisplayFor(modelItem => item.IdEstatusRegistro)
            </td>
            <td style="text-align:center">
                <a asp-action="Edit" asp-route-id="@item.IdClienteContacto">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdClienteContacto">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdClienteContacto">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
